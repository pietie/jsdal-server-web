{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "jsdal-server-web": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.dev.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.png",
              "src/jsdal.json"

            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/font-awesome.min.css",
              "node_modules/codemirror/lib/codemirror.css",
              "node_modules/codemirror/theme/cobalt.css",
              "node_modules/codemirror/addon/lint/lint.css",
              "node_modules/codemirror/addon/hint/show-hint.css",
              "node_modules/codemirror-addon-lint-fix/dist/lint-fix.css",
              "node_modules/mirrorsharp/mirrorsharp.css"
            ],
            "scripts": [
              "bower_components/moment/moment.js",
              "bower_components/humanize-duration/humanize-duration.js",
              "node_modules/ace-builds/src-min/ace.js",
              "node_modules/ace-builds/src-min/mode-csharp.js",
              "node_modules/ace-builds/src-min/theme-merbivore_soft.js"
            ]
          },
          "configurations": {
            "production": {
              "index": "src/index.html",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "jsdal-server-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "jsdal-server-web:build:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "jsdal-server-web",
  "schematics": {
    "@schematics/angular:component": {
      "skipTests": true,
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:guard": {
      "skipTests": true
    },
    "@schematics/angular:pipe": {
      "skipTests": true
    },
    "@schematics/angular:service": {
      "skipTests": true
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
