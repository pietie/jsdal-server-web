{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "jsdal-server-web": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.png"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/font-awesome.min.css"
            ],
            "scripts": [
              "src/assets/js/jquery-2.2.0.min.js",
              "bower_components/moment/moment.js",
              "bower_components/humanize-duration/humanize-duration.js",
              "bower_components/Sortable/Sortable.min.js",
              "node_modules/ace-builds/src-min/ace.js",
              "node_modules/ace-builds/src-min/mode-csharp.js",
              "node_modules/ace-builds/src-min/theme-merbivore_soft.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "jsdal-server-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "jsdal-server-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jsdal-server-web:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "jsdal-server-web-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application"
    }
  },
  "defaultProject": "jsdal-server-web",
  "schematics": {
    "@schematics/angular:component": {
      "spec": false,
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:guard": {
      "spec": false
    },
    "@schematics/angular:pipe": {
      "spec": false
    },
    "@schematics/angular:service": {
      "spec": false
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}