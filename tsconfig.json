{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "./src/app/*"
      ],
      "l2-lib/*": [
        "./node_modules/l2-lib/dist/*"
      ],
      "jsdal-api": [
        "./src/app/services/api"
      ]
    },
    "declaration": false,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "noLib": false,
    "mapRoot": "./",
    "module": "es2020",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "importHelpers": true,
    "target": "ES2022",
    "types": [],
    "typeRoots": [
      "node_modules/@types",
      "node_modules/mirrorsharp"
    ],
    "useDefineForClassFields": false
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
